{
  "name": "saoviet-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "docs:app": "npx @compodoc/compodoc -p tsconfig.json -s -r 8078 -d .documentation/app --includes ./documents",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --builder swc --type-check --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node -r tsconfig-paths/register dist/main",
    "db:dev": "yarn prisma dev",
    "db:reset": "yarn prisma migrate reset",
    "db:generate": "yarn prisma generate",
    "db:migrate": "yarn prisma migrate dev",
    "db:migrate:init": "yarn prisma migrate dev --name init",
    "db:studio": "yarn prisma studio",
    "db:pull": "yarn prisma db pull",
    "db:push": "yarn prisma db push",
    "db:validate": "yarn prisma validate",
    "db:format": "yarn prisma format",
    "db:version": "yarn prisma version",
    "db:debug": "yarn prisma debug",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "dev:local": "npm run dev:services && npm run db:generate && npm run dev",
    "dev:setup": "npm run dev:services && npm run db:generate && npm run db:seed && npm run db:seed:admin",
    "dev:services": "docker-compose -f docker-compose.dev.yml up -d postgres redis",
    "dev:services:down": "docker-compose -f docker-compose.dev.yml down",
    "dev:services:up": "docker-compose -f docker-compose.dev.yml up -d",
    "dev:services:logs": "docker-compose -f docker-compose.dev.yml logs -f postgres redis",
    "dev:pgadmin": "docker-compose -f docker-compose.dev.yml up -d pgadmin",
    "dev:redis-cli": "docker-compose -f docker-compose.dev.yml exec redis redis-cli",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:dev:build": "docker-compose -f docker-compose.dev.yml up -d --build",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:dev:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "docker:prod": "docker-compose up -d",
    "docker:prod:build": "docker-compose up -d --build",
    "docker:prod:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f app"
  },
  "dependencies": {
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.19.0",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.1",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.63.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "csrf-csrf": "^4.0.3",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "js-yaml": "^4.1.0",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.10",
    "otplib": "^12.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1",
    "ua-parser-js": "^2.0.6",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.32",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@suites/di.nestjs": "^3.0.1",
    "@suites/doubles.jest": "^3.0.1",
    "@suites/unit": "^3.0.1",
    "@swc/jest": "^0.2.39",
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.0",
    "@types/ioredis-mock": "^8.2.6",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.7",
    "@types/nodemailer": "^7.0.3",
    "@types/supertest": "^6.0.2",
    "@types/ua-parser-js": "^0.7.39",
    "@types/uuid": "^11.0.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "ioredis-mock": "^8.13.1",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "prisma": "^6.19.0",
    "prisma-dbml-generator": "^0.12.0",
    "prisma-openapi": "^1.3.6",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "@swc/jest"
      ]
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": "^18.14.0 || ^20.0.0 || ^22.0.0 || >=24.0.0"
  }
}
